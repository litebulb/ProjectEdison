{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "ServiceBusRabbitMQ": {
    "Uri": "rabbitmq://abi-rabbit"
  },
  "ServiceBusAzure": {
    "OperationTimeoutSeconds": 5
  },
  "AzureServiceBus": {
    "PrefetchCount": "8"
  },
  "MessageDispatcher": {
    "DispatcherEnabled": true,
    "DefaultMessageType": "eventDevice",
    "DefaultEventType": "button",
    "PropertyDeviceId": [
      "iothub-connection-device-id"
    ],
    "PropertyEventType": [
      "eventType",
      "iothub-message-schema"
    ],
    "PropertyMessageType": [
      "messageType",
      "opType"
    ]
  }
}